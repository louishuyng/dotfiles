{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "Lua.diagnostics.disable": [
    "different-requires",
    "redefined-local",
    "unused-local",
    "cast-local-type",
        "duplicate-index",
        "undefined-global",
        "deprecated",
        "assign-type-mismatch"
  ],
  "diagnostics.globals": [
    "vim",
    "describe",
    "it",
    "before_each",
    "after_each",
    "_lazygit_toggle"
  ]
}
