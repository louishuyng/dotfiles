{
  "workbench.colorTheme": "Catppuccin Frapp√©",
  "yaml.schemas": {
    "file:///Users/louishuyng/.windsurf/extensions/atlassian.atlascode-3.4.12-universal/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "editor.fontFamily": "ui-monospace",
  "editor.fontSize": 13,
  "redhat.telemetry.enabled": true,
  "editor.accessibilitySupport": "off",
  "editor.minimap.enabled": false,
  "vim.leader": ",",
  "vim.normalModeKeyBindings": [
    {
      "before": [
        ",",
        "v"
      ],
      "commands": [
        "workbench.action.splitEditorRight"
      ]
    },
    {
      "before": [
        "<space>",
        "w"
      ],
      "commands": [
        "workbench.action.files.save"
      ]
    },
    {
      "before": [
        "<space>",
        "q"
      ],
      "commands": [
        "workbench.action.closeActiveEditor"
      ]
    },
    {
      "before": [
        "<space>",
        "Q"
      ],
      "commands": [
        "workbench.action.closeWindow"
      ]
    },
    {
      "before": [
        "<space>",
        "<space>"
      ],
      "commands": [
        "workbench.action.quickOpenView"
      ]
    },
    {
      "before": [
        "<space>",
        "/"
      ],
      "commands": [
        "workbench.action.findInFiles"
      ]
    },
    {
      "before": [
        "<leader>",
        "g",
        "s"
      ],
      "commands": [
        "workbench.view.scm"
      ]
    },
    {
      "before": [
        "<leader>",
        "w"
      ],
      "commands": [
        "workbench.action.nextEditor"
      ]
    },
    {
      "before": [
        "<leader>",
        "q"
      ],
      "commands": [
        "workbench.action.previousEditor"
      ]
    },
    {
      "before": [
        "g",
        "f"
      ],
      "commands": [
        "editor.action.goToDeclaration"
      ]
    },
    {
      "before": [
        "g",
        "r"
      ],
      "commands": [
        "editor.action.goToReferences"
      ]
    },
    {
      "before": [
        "g",
        "v"
      ],
      "commands": [
        "workbench.action.splitEditorRight",
        "editor.action.goToDeclaration"
      ]
    },
    {
      "before": [
        "g",
        "s"
      ],
      "commands": [
        "workbench.action.splitEditorDown",
        "workbench.action.nextEditor"
      ]
    },
    {
      "before": [
        ",",
        "b",
        "d"
      ],
      "commands": [
        "workbench.action.closeActiveEditor"
      ]
    },
    {
      "before": [
        ":",
        "w"
      ],
      "commands": [
        "workbench.action.files.save"
      ]
    },
    {
      "before": [
        "<leader>",
        "a"
      ],
      "commands": [
        "extension.jumpToAlternative"
      ]
    },
    {
      "before": [
        "<space>",
        "d",
        "b"
      ],
      "commands": [
        "editor.debug.toggleBreakpoint"
      ]
    },
    {
      "before": [
        "<space>",
        "t",
        "f"
      ],
      "commands": [
        "extension.runJest"
      ]
    },
    {
      "before": [
        "<space>",
        "t",
        "l"
      ],
      "commands": [
        "extension.runPrevJest"
      ]
    }
  ]
}
